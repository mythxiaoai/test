		.tjbox {height:600px;margin:20px auto 0;}
		.tjbox li.items{width:280px;height:300px;float:left;margin-right:20px;margin-bottom:20px;}/*1180/4*-4*20，浮动流体发生改变 margin-top就有折射点*/
		.tjbox li{position:relative;}
		.tjbox li.last{margin-right:0}
		.tjbox li.items:before{content:"";position:absolute;background:#fff;opacity:0;left:-14px;right:-14px;top:-16px;bottom:0px;z-index:-1;box-shadow: 0 0 12px rgba(0, 0, 0, 0.1);}
		.tjbox .items:hover:before {opacity:1;}
		.tjbox .items:hover strong{color:#3498db}
		.tjbox .items:hover .show-img img{transition:all .3s;transform:scale(1.1);}
		.tjbox .items .show-img{display:block;overflow:hidden;width:280px;height:210px;}
		.tjbox li.items h3{padding:5px 0;font-weight:700;color:#34495e;font-size:14px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden}
		.tjbox li.items{font-size:14px;color:#9aabb8;  }
		.tjbox li.items .creat{margin-right: 10px;
    display: inline-block;padding:0 5px;font-size:12px;height:20px;color:#9aabb8;line-height:20px;background:#dfe5e9;text-align:center;border-radius:3px;}
		.tjbox li.items i{padding:0 10px;font-size:18px;color:#9aabb8;}
		.tjbox li.items .pic{height:20px;padding:7px 0 0 0;}
		.tjbox li.items .hpic{height:18px;width:18px;border-radius:50%;margin-right:5px;}
		.tjbox li.items strong{font-size:12px; color:#2c3e50;}


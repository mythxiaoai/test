<script type="text/javascript" src="js/jquery-1.11.1.min.js"></script>
<script type="text/javascript">
	$(function(){
	//获取页面session
	if(sessionStorage){
	var myth_session = sessionStorage.getItem("myth_session");
	//var myth_session = localStorage.getItem("myth_session");
	}
	if(myth_session){
		$(".t_all").html(myth_session);
	}

	//图片加载到输入框
		//1.点击触发表情包显示和隐藏
	$(".t_gif").click(function(){
		$(".t_facebox").toggle(500);
	});

		//2.点击加载到文本框
	$(".t_faceul li").click(function(){
		var img=$(this).html();
		$(".t_message").append(img);
		$(".t_facebox").hide(500);
	});

	//点击发表的动态的过程
		//1.点击发表判断是否为空
		$(".t_btn").click(function(){
			var content=$(".t_message").text().trim()!=""?true:false;
			var img=$(".t_message").find("img").html()!=null?true:false;
			if(content||img){
		//2.不为空才发表  下面的t_all添加标签,并清楚当前编辑内容
				$(".t_all").prepend(
				"<div class='t_list animated bounceIn '>"+
				"<div class='t_head fl'><img  src='images\\me.png' alt='帅帅的小艾' width='66' height='66'><img></div>"+
				"<div class='t_icon fl'></div>"+
				"<div class='t_msg fl'>"+$(".t_message").html()+"</div>"+
				"</div>");
				$(".t_message").text("");
		//3.操作玩后将回话保存到session中本地化 
		// sessionStorage会话结束才消失是html5本地化的新的会话机制
			sessionStorage.setItem("myth_session",$(".t_all").html());
		// localStorage 存到本地，浏览器关闭时，任然存在
			//localStorage.setItem("myth_session",$(".t_all").html());
			}else{
				alert("不能为空或空串哦^_^!请重新输入~");
				$(".t_message").focus();
				return;
			}
		});
	});
</script>

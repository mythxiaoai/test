		<style type="text/css">
			/*公用样式*/
			*{padding:0;margin:0;}
			img{border:none}
			.fl{float:left;}
			.fr{float:right;}
			/*body的背景图片*/
			body{background:url("images/1.jpg") no-repeat bottom center fixed;background-size:cover;font-size:12px;color:#666;font-family:"微软雅黑"}
			/*timeLine Start 线的样式*/
			.timeLine{height:500px;padding:63px 0 0 166px;position:relative;}
			.timeLine .t_line{width:5px;background:linear-gradient(to top,#f03 0% ,#99f 30%,#ff3 80%); height:960px;position:absolute;top:130px;left:200px;box-shadow:1px 0px 2px #ff3;}
			/*end timeLine*/

			/*t_send start 消息框*/
			.timeLine .t_send{height:200px;width:700px;}/*图片中间=200-33+2   图片离时光轴偏左1px；*/
			.timeLine .t_send .t_head{height:66px;width:66px;}
			.timeLine .t_send .t_head img{border-radius:50%;}
			.timeLine .t_send .t_icon{display:inline-block;border:15px solid rgba(0,0,0,0);border-right:15px solid #fff;margin: 14px 0 0 0px;}
			.timeLine .t_send .t_box{height:165px;width:600px;border-radius:5px;background:#fff;position:relative;}
			.timeLine .t_send .t_box .t_tittle{padding: 15px 0 10px 15px;font-size:14px;}
			.timeLine .t_send .t_box .t_message{height:55px;width:540px;border:1px solid #ccc;font-size:14px;line-height:16px;overflow:auto;padding:10px;margin-left: 15px;}
			.timeLine .t_send .t_box .t_message img{vertical-align: middle;margin:2px;}
			.timeLine .t_send .t_box .t_option{height:40px;padding:6px 0 0 15px;}
			.timeLine .t_send .t_box .t_option .t_gif {display:inline-block;border:1px solid #fff;padding-top: 5px;}
			.timeLine .t_send .t_box .t_option .t_gif a{text-decoration:none;color:#fff;}
			.timeLine .t_send .t_box .t_option .t_gif:hover{border:1px solid #ccc;}
			.timeLine .t_send .t_box .t_option .t_btn{display:block;height:32px;width:100px;background:#5caae6;color:#fff;text-align:center;line-height:32px;margin-right:30px;box-shadow:10px;text-decoration:none;border-radius:3px;}
			.timeLine .t_send .t_box .t_option .t_btn:hover{background:#09f}
			/*end t_send 消息框结束*/

			/*t_facebox start 表情包*/
			.timeLine .t_facebox{background:#fff;width:365px;background:#fff;position:absolute;top:149px;left:29px;z-index:1; border-radius:1px;box-shadow:1px 3px 10px 2px;display:none;}
			.timeLine .t_facebox .t_faceul{padding:1px;overflow:hidden;border:1px solid #ccc;}
			.timeLine .t_facebox .t_faceul li{list-style:none;float:left;border:1px solid #fff;padding:3px;cursor:pointer;}
			.timeLine .t_facebox .t_faceul li:hover{border:1px solid #999;}
			/*end t_facebox*/

			/*list start 列表*/
			.timeLine .t_all .t_list{margin:30px 0 30px 0px;}
			.timeLine .t_all .t_list:after{content:"";display:block;clear: both;}
			.timeLine .t_all .t_head{height:66px;width:66px;}
			.timeLine .t_all .t_head img{border-radius:50%;}
			.timeLine .t_all .t_icon{display:inline-block;border:15px solid rgba(0,0,0,0);border-right:15px solid #fff;margin: 14px 0 0 0px;}
			.timeLine .t_all .t_msg{width:500px;background:#fff;border-radius:5px;padding:20px;font-size:14px;}
			.timeLine .t_all .t_msg img{vertical-align: middle;margin:2px;}
			/*end start*/

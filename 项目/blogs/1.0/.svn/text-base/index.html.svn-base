				if($("#main").width()==180){
				$("#main").animate({"width":50},300);
					if($("#sub").offset().left==0){
					$("#sub").stop(true,true).animate({"left":-130},300);
					}else{
					$("#sub").stop(true,true).animate({"left":50},300);
					}
				}else if($("#main").width()==50){
				$("#main").stop(true,true).animate({"width":180},300);
					if($("#sub").offset().left==-130){
					$("#sub").stop(true,true).animate({"left":0},300);
					}else{
					$("#sub").stop(true,true).animate({"left":180},300);
					}
				}
			  });
			//2.子菜单缩放
			$(".sub_icon_close").click(function(){
				if($("#main").width()==180){
					$("#sub").animate({"left":0},300,function(){
						$(".sub_icon_close").hide();
						$(".sub_icon_open").show();
					})
				}else{
						$("#sub").animate({"left":-130},300,function(){
						$(".sub_icon_close").hide();
						$(".sub_icon_open").show();
					})
				}
			})
			$(".sub_icon_open").click(function(){
				if($("#main").width()==180){
					$("#sub").animate({"left":180},300,function(){
						$(".sub_icon_open").hide();
						$(".sub_icon_close").show();
					})
				}else{
					$("#sub").animate({"left":50},300,function(){
						$(".sub_icon_open").hide();
						$(".sub_icon_close").show();
					})
				}
			})
			//选项点击
			$("#main_list .items").each(function(index,val){
				$(this).click(function(){
					var text=$(this).text();
					if(index==1||index==2){
					alert("有待开发~")
					}
					$(this).find(".item").addClass("on").end().siblings().find(".item").removeClass("on");
					$(this).find(".iconfont").addClass("active").end().siblings().find(".iconfont").removeClass("active");
					if(index==0){
						$(".sub_icon_open").trigger("click");
						$("#sub").show();
						$("#sub_list").show();
						$("#sub_tool").hide();
						$("#sub .title").html(text);
						//模板数据加载
					}
					if(index==3){
						$(".sub_icon_open").trigger("click");
						$("#sub").show();
						$("#sub_tool").show();
						$("#sub_list").hide();
						$("#sub .title").html(text);
					}
				});
			});
			//子导航点击样式和动画控制
			$("#sub_list .items,#sub_tool .items").each(function(index,val){
				var num =$(this).find(".sub_ul2").children().length;
				$(this).find(".num").html(num);
				$(this).click(function(){
					$(this).find(".itemA").addClass("on").end().siblings().find(".itemA").removeClass("on");
					
					$(this).find(".sub_ul2").stop(true,true).slideToggle().end().siblings().find(".sub_ul2").slideUp();
					
				});
				$(this).find(".itemA2").click(function(e){
					$(".itemA2").removeClass("on2");
					$(this).addClass("on2");
					if($("#main").width()==180){
						$("#main").animate({"width":50},300);
							if($("#sub").offset().left==0){
							$("#sub").stop(true,true).animate({"left":-130},300);
							}else{
							$("#sub").stop(true,true).animate({"left":50},300);
							}
					}
					if($("#sub").width()==180){
						$("#sub").animate({"left":-130},300,function(){
						$(".sub_icon_close").hide();
						$(".sub_icon_open").show();
						});
					}
				$("html,body").animate({"scrollTop":60},300);
				});
			});
		});

		</script>
	</body>
</html>
